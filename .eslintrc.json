{
    "extends":["airbnb"],
    "ignorePatterns": [
        "/node_modules/*"
      ],
      "parserOptions": {
        // Required for certain syntax usages
        "ecmaVersion": 2020
  },
    "rules": {
      "react/jsx-filename-extension": [2, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }],
      "import/extensions":0,
      "import/no-unresolved": 0,
      "comma-dangle":0,
       "react/function-component-definition":0,
       "max-len": [
        "error",
        {
           "code": 170
         }
      ]
     }
}
